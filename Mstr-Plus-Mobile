startSpamButton.MouseButton1Click:Connect(function()
    local cmd = commandBox.Text
    if cmd == "" then return end
    local speed = tonumber(speedBox.Text) or CommandSpamSpeed
    if CommandSpamActive then
        CommandSpamActive = false
        startSpamButton.Text = "âš¡ Ø³Ø¨Ø§Ù…" startSpamButton.BackgroundColor3 = Color3.fromRGB(255, 150, 0)
    else
        CommandSpamActive = true
        local taskId = #spamTasks + 1
        spamTasks[taskId] = task.spawn(function()
            while CommandSpamActive do
                ExecuteCommand(cmd)
                local waitTime = math.clamp(speed, 0.00001, 10)
                task.wait(waitTime)
            end
            spamTasks[taskId] = nil
        end)
        startSpamButton.Text = "âš¡ Ù†Ø´Ø·" startSpamButton.BackgroundColor3 = Color3.fromRGB(0, 200, 100)
    end
end)

