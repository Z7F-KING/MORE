local function ToggleMasterCounter()
    if not MasterCounterGUI then
        CreateMasterCounterGUI()
    end
    
    MasterCounterOpen = not MasterCounterOpen
    MasterCounterGUI.Enabled = MasterCounterOpen
    
    if MasterCounterOpen then
        local function UpdateSelectedPlayersList()
            if not MasterCounterFrame then return end
            
            local playersScroll = MasterCounterFrame:FindFirstChildWhichIsA("ScrollingFrame")
            if not playersScroll then return end
            
            for _, child in ipairs(playersScroll:GetChildren()) do
                if child:IsA("Frame") then
                    child:Destroy()
                end
            end
            
            local playerCount = 0
            for userId, playerData in pairs(MasterCounterSelectedPlayers) do
                playerCount = playerCount + 1
                local playerFrame = Instance.new("Frame", playersScroll)
                playerFrame.Size = UDim2.new(1, -10, 0, 60)
                playerFrame.BackgroundColor3 = Color3.fromRGB(40, 45, 65)
                playerFrame.LayoutOrder = playerCount
                Instance.new("UICorner", playerFrame).CornerRadius = UDim.new(0, 6)
                
                local playerThumbnail = Instance.new("ImageLabel", playerFrame)
                playerThumbnail.Size = UDim2.new(0, 50, 0, 50)
                playerThumbnail.Position = UDim2.new(0, 5, 0, 5)
                playerThumbnail.BackgroundColor3 = Color3.fromRGB(60, 60, 80)
                Instance.new("UICorner", playerThumbnail).CornerRadius = UDim.new(0, 6)
                
                local playerName = Instance.new("TextLabel", playerFrame)
                playerName.Size = UDim2.new(0.5, 0, 0, 25)
                playerName.Position = UDim2.new(0.2, 0, 0, 5)
                playerName.BackgroundTransparency = 1
                playerName.Text = playerData.Name
                playerName.TextColor3 = Color3.fromRGB(255, 255, 255)
                playerName.TextSize = 14
                playerName.Font = Enum.Font.GothamBold
                playerName.TextXAlignment = Enum.TextXAlignment.Left
                
                local leaveCountLabel = Instance.new("TextLabel", playerFrame)
                leaveCountLabel.Size = UDim2.new(0.5, 0, 0, 25)
                leaveCountLabel.Position = UDim2.new(0.2, 0, 0, 30)
                leaveCountLabel.BackgroundTransparency = 1
                leaveCountLabel.Text = "Ø®Ø±ÙˆØ¬ " .. (PlayerLeaveCounts[userId] or 0)
                leaveCountLabel.TextColor3 = Color3.fromRGB(255, 50, 50)
                leaveCountLabel.TextSize = 12
                leaveCountLabel.Font = Enum.Font.GothamBold
                leaveCountLabel.TextXAlignment = Enum.TextXAlignment.Left
                
                local deleteButton = Instance.new("TextButton", playerFrame)
                deleteButton.Size = UDim2.new(0, 30, 0, 30)
                deleteButton.Position = UDim2.new(1, -35, 0, 15)
                deleteButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
                deleteButton.Text = "âœ•"
                deleteButton.TextColor3 = Color3.fromRGB(255, 255, 255)
                deleteButton.TextSize = 16
                deleteButton.Font = Enum.Font.GothamBold
                Instance.new("UICorner", deleteButton).CornerRadius = UDim.new(0, 6)
                
                deleteButton.MouseButton1Click:Connect(function()
                    MasterCounterSelectedPlayers[userId] = nil
                    UpdateSelectedPlayersList()
                end)
                
                pcall(function()
                    local thumb = Players:GetUserThumbnailAsync(userId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
                    playerThumbnail.Image = thumb
                end)
            end
        end
        
        UpdateSelectedPlayersList()
    end
end

