-- Ù†Ø¸Ø§Ù… Ø§Ù„Ø¨Ø­Ø«
local function UpdateSelectedPlayer(searchTerm)
    if searchTerm == "" then
        playerInfoLabel.Text = "ğŸ” Ø§Ø¨Ø­Ø« Ø¹Ù† Ù„Ø§Ø¹Ø¨ Ø¨Ø§Ø³ØªØ®Ø¯Ø§Ù… Ø§Ù„Ø§Ø³Ù… Ø£Ùˆ Ø§Ù„Ø¹Ø±Ø¶ Ø£Ùˆ ID"
        SelectedPlayer = nil
        return
    end
    local foundPlayers = FindPlayer(searchTerm)
    if #foundPlayers == 0 then
        playerInfoLabel.Text = "âŒ Ù„Ø§ ÙŠÙˆØ¬Ø¯ Ù„Ø§Ø¹Ø¨ Ø¨Ù‡Ø°Ø§ Ø§Ù„Ø§Ø³Ù…"
        SelectedPlayer = nil
    elseif #foundPlayers == 1 then
        SelectedPlayer = foundPlayers[1]
        playerInfoLabel.Text = GetPlayerInfo(SelectedPlayer)
    else
        SelectedPlayer = foundPlayers[1]
        playerInfoLabel.Text = GetPlayerInfo(SelectedPlayer) .. string.format("\n\nâš ï¸ ÙŠÙˆØ¬Ø¯ %d Ù„Ø§Ø¹Ø¨ÙŠÙ† Ù…Ø·Ø§Ø¨Ù‚ÙŠÙ†\nğŸ‘¤ ØªÙ… Ø§Ø®ØªÙŠØ§Ø±: %s", #foundPlayers, SelectedPlayer.Name)
    end
end

