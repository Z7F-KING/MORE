local function CreateTabButton(name, text, xPosition)
    local tabButton = Instance.new("TextButton", tabButtonsFrame)
    tabButton.Size = UDim2.new(0.16, 0, 1, 0)
    tabButton.Position = UDim2.new(xPosition, 0, 0, 0)
    tabButton.BackgroundColor3 = Color3.fromRGB(40, 45, 60)
    tabButton.Text = text
    tabButton.TextColor3 = Color3.fromRGB(200, 200, 255)
    tabButton.TextSize = 11
    tabButton.Font = Enum.Font.GothamBold
    tabButton.Name = name
    Instance.new("UICorner", tabButton).CornerRadius = UDim.new(0, 6)
    tabs[name] = Instance.new("ScrollingFrame", contentFrame)
    tabs[name].ScrollBarThickness = 2
    tabs[name].BackgroundTransparency = 1
    tabs[name].CanvasSize = UDim2.new(0, 0, 0, 1000)
    tabs[name].Size = UDim2.new(1, 0, 1, -45)
    tabs[name].Position = UDim2.new(0, 0, 0, 35)
    tabs[name].BackgroundTransparency = 1
    tabs[name].Visible = false
    return tabButton
end

local mainTabBtn = CreateTabButton("main", "ğŸ ", 0)
local playerTabBtn = CreateTabButton("player", "ğŸ‘¤", 0.16)
local commandsTabBtn = CreateTabButton("commands", "ğŸ®", 0.32)
local chatTabBtn = CreateTabButton("chat", "ğŸ’¬", 0.48)
