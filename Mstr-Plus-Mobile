    local info = "ğŸ‘¤ Ø§Ù„Ø§Ø³Ù…: " .. targetPlayer.Name .. "\nğŸ­ Ø§Ù„Ø¹Ø±Ø¶: " .. targetPlayer.DisplayName .. "\nğŸ†” ID: " .. tostring(targetPlayer.UserId)
    if IsNANPlayer(targetPlayer) then
        info = info .. "\n\nâš ï¸ ğŸš« ØªØ­Ø°ÙŠØ±: Ù‡Ø°Ø§ Ø§Ù„Ù„Ø§Ø¹Ø¨ NAN!\nâŒ Ù„Ø§ ÙŠÙ…ÙƒÙ†Ùƒ Ø§Ø³ØªØ®Ø¯Ø§Ù… Ø§Ù„Ø£ÙˆØ§Ù…Ø± Ø§Ù†Ø³Ø® Ø¹Ù„ÙŠÙ‡ Ùˆ Ø¹Ù„Ù‚Ù‡!\nâœ… ÙŠÙ…ÙƒÙ†Ùƒ Ø§Ø³ØªØ®Ø¯Ø§Ù… Ø¨Ø§Ù‚ÙŠ Ø§Ù„Ø£ÙˆØ§Ù…Ø±"
    end
    if count > 0 then info = info .. "\n\nğŸš« Ù‡Ø±ÙˆØ¨: " .. count .. " Ù…Ø±Ø©" end
