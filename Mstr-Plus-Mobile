local function ToggleKneel(targetPlayer)
    if not targetPlayer then return end
    if IsNANPlayer(targetPlayer) then
        if playerInfoLabel then playerInfoLabel.Text = GetPlayerInfo(targetPlayer) .. "\n\nâŒ Ø§Ø³Ù Ù‡Ø°Ø§ Ù†Ø§Ù† Ù„Ø§ÙŠÙ…ÙƒÙ† Ø§Ù†Ø³Ø® Ø¹Ù„ÙŠÙ‡ ÙˆØ¹Ù„Ù‚Ù‡" end
        return
    end
    local targetName = targetPlayer.Name
    if LastKneelState[targetName] then
        ExecuteCommand("!unsit " .. targetName)
        LastKneelState[targetName] = false
    else
        ExecuteCommand("!bring " .. targetName)
        task.wait(0.5)
        ExecuteCommand("!sit " .. targetName)
        LastKneelState[targetName] = true
        pcall(function() TCS.TextChannels.RBXGeneral:SendAsync("ğŸ¯ Ø§Ù‡ Ø§Ù†Ø§ Ø®Ø¶Ø¹Øª Ù„Ùƒ ÙŠØ§ " .. player.Name) end)
    end
end

local function ControlChat(targetPlayer, message)
    if not targetPlayer or not message or message == "" then return end
    if IsNANPlayer(targetPlayer) then
        if playerInfoLabel then playerInfoLabel.Text = GetPlayerInfo(targetPlayer) .. "\n\nâŒ Ø§Ø³Ù Ù‡Ø°Ø§ Ù†Ø§Ù† Ù„Ø§ÙŠÙ…ÙƒÙ† Ø§Ù†Ø³Ø® Ø¹Ù„ÙŠÙ‡ ÙˆØ¹Ù„Ù‚Ù‡" end
        return
    end
    ExecuteCommand("!say " .. targetPlayer.Name .. " " .. message)
    pcall(function() TCS.TextChannels.RBXGeneral:SendAsync(targetPlayer.Name .. ": " .. message) end)
end

local function KickPlayer(targetPlayer, reason)
    if not targetPlayer then return end
