antiBangButton.MouseButton1Click:Connect(function()
    if not SelectedPlayer then
        playerInfoLabel.Text = "âŒ Ø§Ù„Ø±Ø¬Ø§Ø¡ ØªØ­Ø¯ÙŠØ¯ Ù„Ø§Ø¹Ø¨ Ø£ÙˆÙ„Ø§Ù‹"
        return
    end
    if not AntiBangLoopActive then
        AntiBangLoopActive = true
        antiBangButton.Text = "ğŸ”„ Ø¥ÙŠÙ‚Ø§Ù"
        antiBangButton.BackgroundColor3 = Color3.fromRGB(220, 60, 60)
        AntiBangLoopTask = task.spawn(function()
            local currentIndex = 1
            while AntiBangLoopActive do
                local playerRoot = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
                if playerRoot then
                    playerRoot.CFrame = AntiBangLocations[currentIndex]
                    task.wait(0.1) -- ØªØºÙŠÙŠØ± Ø§Ù„Ø³Ø±Ø¹Ø© Ø¥Ù„Ù‰ 0.1
                    currentIndex = currentIndex + 1
                    if currentIndex > #AntiBangLocations then currentIndex = 1 end
                else task.wait(1) end
            end
        end)
    else
        AntiBangLoopActive = false
        antiBangButton.Text = "ØªÙØ¹ÙŠÙ„"
        antiBangButton.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
    end
end)

