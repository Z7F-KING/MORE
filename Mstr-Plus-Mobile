closeButton.MouseButton1Click:Connect(function()
    CommandSpamActive, ChatSpamActive = false, false
    StopBang() StopView() StopAntiCopy() StopCopyCommands() StopHangCommands()
    AntiBangLoopActive = false
    if AntiKillActive and AntiKillOriginalPosition then
        local playerRoot = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
        if playerRoot then playerRoot.CFrame = AntiKillOriginalPosition end
    end
    if afkConnection then afkConnection:Disconnect() end
    if MasterCounterGUI then MasterCounterGUI:Destroy() end
    mainGUI:Destroy()
end)

Players.PlayerRemoving:Connect(function(removingPlayer)
    -- ØªØ­Ø¯ÙŠØ« Ø¹Ø¯Ø§Ø¯ Ø§Ù„Ù„Ø§Ø¹Ø¨ Ø§Ù„Ù…Ø­Ø¯Ø¯ ÙÙŠ Ø§Ù„ØªØ¨ÙˆÙŠØ¨ Ø§Ù„Ø±Ø¦ÙŠØ³ÙŠ
    if SelectedPlayer == removingPlayer then
        PlayerLeaveCounts[removingPlayer.UserId] = (PlayerLeaveCounts[removingPlayer.UserId] or 0) + 1
        local count = PlayerLeaveCounts[removingPlayer.UserId]
        ShowNotification("Ø®Ø±Ø¬ Ø§Ù„Ù„Ø§Ø¹Ø¨", string.format("ğŸšª Ø§Ù„Ù„Ø§Ø¹Ø¨ %s Ø®Ø±Ø¬ Ù…Ù† Ø§Ù„Ø³ÙŠØ±Ù\nØ¹Ø¯Ø¯ Ù…Ø±Ø§Øª Ø§Ù„Ù‡Ø±ÙˆØ¨: %d Ù…Ø±Ø©", removingPlayer.Name, count))
        SelectedPlayer = nil
    end
    
    -- ØªØ­Ø¯ÙŠØ« Ø¹Ø¯Ø§Ø¯ Ø§Ù„Ù„Ø§Ø¹Ø¨ Ø§Ù„Ù…Ø­Ø¯Ø¯ ÙÙŠ ØªØ¨ÙˆÙŠØ¨ Ø§Ù„Ø¨Ù„Ø³
    if SelectedPlayerMembership == removingPlayer then
        plusPlayerInfo.Text = string.format("âŒ Ø§Ù„Ù„Ø§Ø¹Ø¨ ØºØ§Ø¯Ø± Ø§Ù„Ø³ÙŠØ±ÙØ±: %s", removingPlayer.Name)
        SelectedPlayerMembership = nil
    end
    
    -- ØªØ­Ø¯ÙŠØ« Ø¹Ø¯Ø§Ø¯ Ø§Ù„Ù„Ø§Ø¹Ø¨ÙŠÙ† ÙÙŠ Ø¹Ø¯Ø§Ø¯ Ù…Ø³ØªØ±
    if MasterCounterSelectedPlayers[removingPlayer.UserId] then
        PlayerLeaveCounts[removingPlayer.UserId] = (PlayerLeaveCounts[removingPlayer.UserId] or 0) + 1
        if MasterCounterOpen and MasterCounterFrame then
            local function UpdateSelectedPlayersList()
                if not MasterCounterFrame then return end
                
                local playersScroll = MasterCounterFrame:FindFirstChildWhichIsA("ScrollingFrame")
                if not playersScroll then return end
                
                for _, child in ipairs(playersScroll:GetChildren()) do
                    if child:IsA("Frame") then
                        child:Destroy()
                    end
                end
                
                local playerCount = 0
                for userId, playerData in pairs(MasterCounterSelectedPlayers) do
                    playerCount = playerCount + 1
                    local playerFrame = Instance.new("Frame", playersScroll)
                    playerFrame.Size = UDim2.new(1, -10, 0, 60)
                    playerFrame.BackgroundColor3 = Color3.fromRGB(40, 45, 65)
                    playerFrame.LayoutOrder = playerCount
                    Instance.new("UICorner", playerFrame).CornerRadius = UDim.new(0, 6)
                    
                    local playerThumbnail = Instance.new("ImageLabel", playerFrame)
                    playerThumbnail.Size = UDim2.new(0, 50, 0, 50)
                    playerThumbnail.Position = UDim2.new(0, 5, 0, 5)
                    playerThumbnail.BackgroundColor3 = Color3.fromRGB(60, 60, 80)
                    Instance.new("UICorner", playerThumbnail).CornerRadius = UDim.new(0, 6)
                    
                    local playerName = Instance.new("TextLabel", playerFrame)
                    playerName.Size = UDim2.new(0.5, 0, 0, 25)
                    playerName.Position = UDim2.new(0.2, 0, 0, 5)
                    playerName.BackgroundTransparency = 1
                    playerName.Text = playerData.Name
                    playerName.TextColor3 = Color3.fromRGB(255, 255, 255)
                    playerName.TextSize = 14
                    playerName.Font = Enum.Font.GothamBold
                    playerName.TextXAlignment = Enum.TextXAlignment.Left
                    
                    local leaveCountLabel = Instance.new("TextLabel", playerFrame)
                    leaveCountLabel.Size = UDim2.new(0.5, 0, 0, 25)
                    leaveCountLabel.Position = UDim2.new(0.2, 0, 0, 30)
                    leaveCountLabel.BackgroundTransparency = 1
                    leaveCountLabel.Text = "Ø®Ø±ÙˆØ¬ " .. (PlayerLeaveCounts[userId] or 0)
                    leaveCountLabel.TextColor3 = Color3.fromRGB(255, 50, 50)
                    leaveCountLabel.TextSize = 12
                    leaveCountLabel.Font = Enum.Font.GothamBold
                    leaveCountLabel.TextXAlignment = Enum.TextXAlignment.Left
                    
                    local deleteButton = Instance.new("TextButton", playerFrame)
                    deleteButton.Size = UDim2.new(0, 30, 0, 30)
                    deleteButton.Position = UDim2.new(1, -35, 0, 15)
                    deleteButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
                    deleteButton.Text = "X"
                    deleteButton.TextColor3 = Color3.fromRGB(255, 255, 255)
                    deleteButton.TextSize = 16
                    deleteButton.Font = Enum.Font.GothamBold
                    Instance.new("UICorner", deleteButton).CornerRadius = UDim.new(0, 6)
                    
                    deleteButton.MouseButton1Click:Connect(function()
                        MasterCounterSelectedPlayers[userId] = nil
                        UpdateSelectedPlayersList()
                    end)
                    
                    pcall(function()
                        local thumb = Players:GetUserThumbnailAsync(userId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
                        playerThumbnail.Image = thumb
                    end)
                end
            end
            
            UpdateSelectedPlayersList()
        end
    end
end)

mainTabBtn.MouseButton1Click:Connect(function() SwitchTab("main") end)
playerTabBtn.MouseButton1Click:Connect(function() SwitchTab("player") if not SelectedPlayer then playerInfoLabel.Text = "ğŸ” Ø§Ø¨Ø­Ø« Ø¹Ù† Ù„Ø§Ø¹Ø¨ Ø¨Ø§Ø³ØªØ®Ø¯Ø§Ù… Ø§Ù„Ø§Ø³Ù… Ø£Ùˆ Ø§Ù„Ø¹Ø±Ø¶ Ø£Ùˆ ID" end end)
commandsTabBtn.MouseButton1Click:Connect(function() SwitchTab("commands") end)
chatTabBtn.MouseButton1Click:Connect(function() SwitchTab("chat") end)
toolsTabBtn.MouseButton1Click:Connect(function() SwitchTab("tools") end)
plusTabBtn.MouseButton1Click:Connect(function() if MembershipActive then SwitchTab("plus") plusSearchBox.Text = "" end end)

local UIS = game:GetService("UserInputService")
UIS.InputBegan:Connect(function(input) if input.KeyCode == Enum.KeyCode.F1 then mainFrame.Visible = not mainFrame.Visible end end)

player.CharacterAdded:Connect(function()
    if BangActive or HeadBangActive then
        task.wait(1)
        if BangActive then PlayDanceAnimation(BangSpeed)
        elseif HeadBangActive then PlayDanceAnimation(HeadBangSpeed) end
    end
end)

-- Ø¥Ø¹Ø¯Ø§Ø¯Ø§Øª Ø§Ù„Ø­Ù‚ÙˆÙ„
bangSpeedBox.FocusLost:Connect(function() local s = tonumber(bangSpeedBox.Text) BangSpeed = s and math.clamp(s, 1, 10) or 1.5 bangSpeedBox.Text = tostring(BangSpeed) end)
bangDistanceBox.FocusLost:Connect(function() local d = tonumber(bangDistanceBox.Text) BangDistance = d and math.clamp(d, 1, 10000) or 200 bangDistanceBox.Text = tostring(BangDistance) end)
bangHeightBox.FocusLost:Connect(function() local h = tonumber(bangHeightBox.Text) BangHeight = h and math.clamp(h, 0, 10000) or 0 bangHeightBox.Text = tostring(BangHeight) end)
bangOscillationBox.FocusLost:Connect(function() local o = tonumber(bangOscillationBox.Text) BangOscillation = o and math.clamp(o, 1, 10) or 1.5 bangOscillationBox.Text = tostring(BangOscillation) end)

headBangSpeedBox.FocusLost:Connect(function() local s = tonumber(headBangSpeedBox.Text) HeadBangSpeed = s and math.clamp(s, 1, 10) or 1.5 headBangSpeedBox.Text = tostring(HeadBangSpeed) end)
headBangDistanceBox.FocusLost:Connect(function() local d = tonumber(headBangDistanceBox.Text) HeadBangDistance = d and math.clamp(d, 1, 10000) or 200 headBangDistanceBox.Text = tostring(HeadBangDistance) end)
headBangHeightBox.FocusLost:Connect(function() local h = tonumber(headBangHeightBox.Text) HeadBangHeight = h and math.clamp(h, 0, 10000) or 0 headBangHeightBox.Text = tostring(HeadBangHeight) end)
headBangOscillationBox.FocusLost:Connect(function() local o = tonumber(headBangOscillationBox.Text) HeadBangOscillation = o and math.clamp(o, 1, 10) or 1.5 headBangOscillationBox.Text = tostring(HeadBangOscillation) end)

speedBox.FocusLost:Connect(function() local s = tonumber(speedBox.Text) CommandSpamSpeed = s and math.clamp(s, 0.00001, 10) or 1.0 speedBox.Text = tostring(CommandSpamSpeed) end)
chatSpeedBox.FocusLost:Connect(function() local s = tonumber(chatSpeedBox.Text) chatSpeedBox.Text = s and tostring(math.clamp(s, 0.1, 10)) or "1.0" end)
