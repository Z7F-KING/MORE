    local target = Players:FindFirstChild(targetPlayer.Name)
    if target and target.Character then
        local targetHRP = target.Character:FindFirstChild("HumanoidRootPart")
        if targetHRP then
            targetHRP.CFrame = CFrame.new(2147483648, 944401, -2)
            ShowNotification("ğŸŒ€ Ø¥Ø±Ù…ÙŠÙ‡ Ø¨Ø§Ù„Ú¤ÙˆÙŠØ¯", "ØªÙ… ÙƒØ¨Ø´ Ø§Ù„Ù„Ø§Ø¹Ø¨ ÙˆØ¥Ø±Ø³Ø§Ù„Ù‡ Ù„Ù„Ú¤ÙˆÙŠØ¯!")
