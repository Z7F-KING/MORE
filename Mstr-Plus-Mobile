-- ØªØ¹Ø¯ÙŠÙ„ Ø¯Ø§Ù„Ø© ÙƒØ±Ø³ÙŠ Ù…Ø¹ Ø§Ù„Ù‚ÙØ²
local function TeleportToLocationAndBack(cframe, waitTime)
    local playerRoot = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
    if not playerRoot then return end
    local originalPosition = playerRoot.CFrame
    playerRoot.CFrame = cframe
    -- Ø¬Ø¹Ù„ Ø§Ù„Ù„Ø§Ø¹Ø¨ ÙŠÙ‚ÙØ²
    local humanoid = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
    if humanoid then
        humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
    end
    task.wait(waitTime)
    playerRoot.CFrame = originalPosition
end

local function ToggleTeleportLocation(cframe, isActive, originalPositionVarName, button)
    local playerRoot = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
    if not playerRoot then return isActive end
    if not isActive then
        if originalPositionVarName == "AntiKillOriginalPosition" then
            AntiKillOriginalPosition = playerRoot.CFrame
        end
        playerRoot.CFrame = cframe
        isActive = true
        button.Text = button.Text .. " ğŸ”„"
        button.BackgroundColor3 = Color3.fromRGB(0, 200, 100)
    else
        local originalPos = AntiKillOriginalPosition
        AntiKillOriginalPosition = nil
        if originalPos then playerRoot.CFrame = originalPos end
        isActive = false
        button.Text = button.Text:gsub(" ğŸ”„", "")
        button.BackgroundColor3 = Color3.fromRGB(220, 60, 60)
    end
    return isActive
end

