    -- ÙˆØ¸ÙŠÙØ© Ø§Ù„Ø¨Ø­Ø« ÙˆØ¥Ø¶Ø§ÙØ© Ù„Ø§Ø¹Ø¨
    searchButton.MouseButton1Click:Connect(function()
        local searchTerm = searchBox.Text
        if searchTerm == "" then return end
        
        local foundPlayers = FindPlayer(searchTerm)
        for _, foundPlayer in ipairs(foundPlayers) do
            if not MasterCounterSelectedPlayers[foundPlayer.UserId] then
                MasterCounterSelectedPlayers[foundPlayer.UserId] = {
                    Name = foundPlayer.Name,
                    UserId = foundPlayer.UserId
                }
            end
        end
        
        if #foundPlayers > 0 then
            UpdateSelectedPlayersList()
            searchBox.Text = ""
        end
    end)
    
