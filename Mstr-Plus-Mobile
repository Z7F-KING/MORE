    if not target or not target.Character then ShowNotification("âŒ Ø®Ø·Ø£", "Ø§Ù„Ù„Ø§Ø¹Ø¨ ØºÙŠØ± Ù…ÙˆØ¬ÙˆØ¯!") return false end
    local targetHRP = target.Character:FindFirstChild("HumanoidRootPart")
    if not targetHRP then ShowNotification("âŒ Ø®Ø·Ø£", "Ø§Ù„Ù„Ø§Ø¹Ø¨ Ù„ÙŠØ³ Ù„Ø¯ÙŠÙ‡ HumanoidRootPart!") return false end
    local cuff = GetRandomCuff()
    if not cuff then ShowNotification("âŒ Ø®Ø·Ø£", "Ù„Ø§ ØªÙˆØ¬Ø¯ ÙƒÙ„Ø¨Ø´Ø§Øª Ù„Ø¯ÙŠÙƒ!") return false end
    local success, errorMsg = pcall(function()
        cuff.Parent = player.Character
        for _, obj in ipairs(cuff:GetDescendants()) do
            if obj:IsA("RemoteEvent") then
                obj:FireServer(targetHRP)
                return true
            end
        end
        ShowNotification("âŒ Ø®Ø·Ø£", "Ù„Ù… ÙŠØªÙ… Ø§Ù„Ø¹Ø«ÙˆØ± Ø¹Ù„Ù‰ RemoteEvent ÙÙŠ Ø§Ù„ÙƒÙ„Ø¨Ø´Ø©!")
        return false
    end)
    if not success then ShowNotification("âŒ Ø®Ø·Ø£", "ÙØ´Ù„ ÙÙŠ ÙƒØ¨Ø´ Ø§Ù„Ù„Ø§Ø¹Ø¨: " .. tostring(errorMsg)) return false end
    return true
end

local function ThrowToVoid(targetPlayer)
    if not targetPlayer then ShowNotification("âŒ Ø®Ø·Ø£", "Ù„Ù… ÙŠØªÙ… ØªØ­Ø¯ÙŠØ¯ Ù„Ø§Ø¹Ø¨!") return false end
